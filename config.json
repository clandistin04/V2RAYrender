        "network": "ws",
        "security": "tls", // تفعيل TLS
        "tlsSettings": {
          "certificates": [
            {
              "certificateFile": "/etc/v2ray/server.crt", // مسار شهادة SSL
              "keyFile": "/etc/v2ray/server.key" // مسار المفتاح الخاص
            }
          ],
          "alpn": ["http/1.1"], // محاكاة حركة HTTP عادية
          "serverName": "fancy-nollie-koki22-c43ddd00.koyeb.app" // نفس Host المستخدم في العميل
        },
        "wsSettings": {
          "path": "/koki", // نفس المسار المستخدم في العميل
          "headers": {
            "Host": "eishan2-3-118642031119.europe-north1.run.app" // نفس Host المستخدم في العميل
          }
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ],
  "policy": {
    "levels": {
      "8": {
        "connIdle": 300,
        "downlinkOnly": 1,
        "handshake": 4,
        "uplinkOnly": 1
      }
    }
  }
}
