{
  "inbounds": [
    {
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "a4bf6af2-719d-4991-af10-d6aaa88c3bc1", // نفس UUID المستخدم في العميل
            "flow": "", // اتركه فارغًا إذا لم تستخدم XTLS
            "level": 8,
            "email": "user@server" // اختياري
          }
        ],
        "decryption": "none", // ضروري لبروتوكول VLESS
        "fallbacks": [] // يمكن إضافة fallbacks إذا لزم الأمر
